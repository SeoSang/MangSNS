{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    // "eslint:recommended", "plugin:react/recommended"
    "airbnb"
  ],
  "plugins": ["import", "react-hooks"],
  "rules": {
    "no-underscore-dangle": 1
  }
}
